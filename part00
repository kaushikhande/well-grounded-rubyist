require './stacklike.rb'
# require_relative 'stacklike.rb'
class Stack
  include Stacklike
end

s = Stack.new
s.add_to_stack("item one")
s.add_to_stack("item two")
s.add_to_stack("item three")

puts "Stack now has"
puts s.stack
puts "-------------"
puts s.take_from_stack
puts "Stack now has"
puts s.stack
puts "--------------"
